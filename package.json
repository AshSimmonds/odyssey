{
  "name": "@viridia/odyssey",
  "version": "0.0.1",
  "description": "Rocket flight simulator",
  "type": "module",
  "keywords": [
    "rocket"
  ],
  "author": "Talin",
  "license": "MIT",
  "scripts": {
    "build": "tsc && vite build",
    "convert:stars": "ts-node --esm scripts/star-convert.ts",
    "dev": "vite",
    "format": "prettier --config .prettierrc.cjs --write ./src",
    "preview": "vite preview",
    "test:lint": "eslint -c .eslintrc.cjs \"src/**/*.ts?(x)\"",
    "test:unit": "vitest run"
  },
  "devDependencies": {
    "babel-preset-solid": "^1.6.2",
    "binary-parser": "^2.2.1",
    "eslint": "^8.28.0",
    "eslint-plugin-deprecation": "^1.3.3",
    "eslint-plugin-import": "^2.26.0",
    "prettier": "^2.7.1",
    "sass": "^1.56.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.6.4",
    "vite": "^3.2.3",
    "vite-plugin-solid": "^2.4.0",
    "vitest": "^0.25.2"
  },
  "dependencies": {
    "@types/three": "^0.146.0",
    "solid-js": "^1.6.2",
    "three": "^0.146.0"
  }
}
